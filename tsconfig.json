{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES6",
    "moduleResolution": "Node",
    "lib": [
      "ES2020"
    ],
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "target",
    "rootDir": "./src",
  },
  "exclude": [
    "node_modules",
    "target"
  ]
}