{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for node with Typescript",
  "scripts": {
    "clean": "rimraf ./target",
    "compile": "tsc -p tsconfig.prod.json",
    "compile:watch": "tsc -p tsconfig.prod.json -w",
    "lint": "eslint ./src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm-run-all clean lint test compile",
    "start": "npm-run-all clean compile:watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unused-imports": "^1.0.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}